datamod()
{
rm -rf /$sd/log 2>/dev/null
rm -rf /$DATA1/cache 2>/dev/null
rm -rf /$DATA2/{ca-bundle.pem,login-identifier.txt,ProgramBinaryCache,tbslog,vmpcloudconfig.json} 2>/dev/null
rm -rf /$DATA3/{Engine,'Epic Games',Paks} 2>/dev/null
rm -rf /$DATA4/{Content,Intermediate} 2>/dev/null
rm -rf /$DATA5/{GameErrorNoRecords,Logs,Pandora,PufferTmpDir,rawdata,RoleInfo,UpdateInfo} 2>/dev/null
rm -rf /$PAKS/*cures*
rm -rf /$PAKS/{apollo_reslist.flist,filelist.json,new.filelist,puffer_res.eifs,puffer_temp,PufferFileList.json} 2>/dev/null
rm -rf /data/dalvik-cache/arm/data@app@com.tencent.ig-2@base.apk@classes.dex 2>/dev/null
mkdir -p /storage/emulated/0/Android/data/com.tencent.ig/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/Paks
touch /$sd/log 2>/dev/null
touch /$DATA1/cache 2>/dev/null
mkdir -p /$DATA2/{ca-bundle.pem,login-identifier.txt,vmpcloudconfig.json} 2>/dev/null
touch /$DATA2/ProgramBinaryCache 2>/dev/null
touch /$DATA2/tbslog 2>/dev/null
touch /$DATA3/Engine 2>/dev/null
touch /$DATA3/'Epic Games' 2>/dev/null
touch /$DATA3/Paks 2>/dev/null
touch /$DATA4/Content 2>/dev/null
touch /$DATA4/Intermediate 2>/dev/null
touch /$DATA5/Logs 2>/dev/null
touch /$DATA5/Pandora 2>/dev/null
touch /$DATA5/PufferTmpDir 2>/dev/null
touch /$DATA5/rawdata 2>/dev/null
touch /$DATA5/RoleInfo 2>/dev/null
touch /$DATA5/UpdateInfo 2>/dev/null
